{
    "extends": [
        "tslint:latest",
        "tslint-react",
        "tslint-config-prettier"
    ],
    "rules": {
        "jsx-no-lambda": false,
        "no-submodule-imports": false,
        "only-arrow-functions": true,
        "arrow-return-shorthand": true,
        "no-trailing-whitespace": true,
        "triple-equals": true,
        "switch-default": true,
        "no-null-keyword": true,
        "curly": true,
        "no-implicit-dependencies": [true, "dev"]
    }
}