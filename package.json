    "@navikt/familie-visittkort": "^15.1.3",
    "@navikt/flagg-ikoner": "^10.0.0",
    "@navikt/fnrvalidator": "^2.1.0",
    "@navikt/land-verktoy": "^9.1.8",
    "@navikt/landvelger": "^10.0.0",
    "@sentry/browser": "^9.29.0",
    "@sentry/webpack-plugin": "^3.5.0",
    "@tanstack/react-query": "^5.80.2",
    "axios": "^1.9.0",
    "babel-loader": "^10.0.0",
    "body-parser": "^2.2.0",
    "classnames": "^2.5.1",
    "date-fns": "4.1.0",
    "deep-equal": "^2.2.0",
    "dotenv": "^16.5.0",
