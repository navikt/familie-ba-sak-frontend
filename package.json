    "webpack-dev-middleware": "^7.3.0",
    "webpack-hot-middleware": "^2.26.1",
    "webpack-merge": "^6.0.1"
  },
  "jest": {
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsconfig": "<rootDir>/src/frontend/tsconfig.json"
      }
    },
    "moduleNameMapper": {
      "\\.(css|svg)$": "<rootDir>/src/mockFile.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(@navikt)/)"
    ],
    "transform": {
      "\\.[jt]sx?$": "babel-jest"
    }
  },
  "resolutions": {
    "styled-components": "^6"
  }
}
