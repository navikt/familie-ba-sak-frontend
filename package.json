{
  "name": "familie-ba-sak-frontend",
  "type": "module",
  "version": "0.0.1",
  "main": "backend/server.js",
  "repository": "https://github.com/navikt/familie-ba-sak-frontend.git",
  "author": "Henning HÃ¥konsen",
  "license": "MIT",
  "scripts": {
    "start": "yarn set-node-options NODE_ENV=production node backend/backend/server.js",
    "start:dev": "tsc -p tsconfig.backend.json && yarn set-node-options NODE_ENV=development node backend/backend/server.js",
    "build": "yarn build:prod",
    "build:prod": "tsc -p tsconfig.backend.json && NODE_ENV=production webpack --config ./src/webpack/webpack.prod.js",
    "test": "jest",
    "test:one": "jest -t",
    "lint": "eslint './src/**/*.{js,ts,tsx}' --max-warnings=0",
    "lint:fix": "eslint './src/**/*.{js,ts,tsx}' --fix",
    "prettier": "prettier --check './src/**/*.{js,jsx,ts,tsx,json,css}'",
    "prettier-check": "prettier --check './src/**/*.{js,jsx,ts,tsx,json,css}'",
    "prettier-fix": "prettier --write './src/**/*.{js,jsx,ts,tsx,json,css}'",
    "prepare": "husky",
    "set-node-options": "NODE_OPTIONS='--import=./backend/backend/register.js --es-module-specifier-resolution=node'",
    "start:cypress": "tsc -p tsconfig.backend.json && yarn set-node-options NODE_ENV=production node backend/backend/cypress-server.js",
    "run:cypress": "cypress run",
    "cypress": "yarn start:cypress & yarn run:cypress",
    "typecheck": "tsc --noEmit -p src/frontend/tsconfig.json  --skipLibCheck && tsc --noEmit -p src/backend/tsconfig.json  --skipLibCheck",
    "validate": "yarn typecheck && yarn lint && yarn prettier"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css}": [
      "prettier --check",
      "eslint --max-warnings=0"
    ]
  },
  "dependencies": {
    "@grafana/faro-web-sdk": "^1.18.0",
