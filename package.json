  "scripts": {
    "start": "NODE_ENV=production node --loader ts-node/esm --es-module-specifier-resolution=node node_dist/backend/server.js",
    "start:dev": "tsc -p tsconfig.backend.json && NODE_ENV=development node --loader ts-node/esm --es-module-specifier-resolution=node node_dist/backend/server.js",
    "build": "yarn build:prod",
    "build:prod": "tsc -p tsconfig.backend.json && NODE_ENV=production webpack --config ./src/webpack/webpack.prod.js",
    "test": "jest",
    "test:one": "jest -t",
    "lint": "eslint './src/**/*.{js,ts,tsx}' --max-warnings=0",
    "lint:fix": "eslint './src/**/*.{js,ts,tsx}' --fix",
    "prettier-check": "prettier --check .",
    "prettier-fix": "prettier --write .",
    "prepare": "husky install",
    "start:cypress": "tsc -p tsconfig.backend.json && NODE_ENV=production node --loader ts-node/esm --es-module-specifier-resolution=node node_dist/backend/cypress-server.js",
    "run:cypress": "cypress run",
    "cypress": "npm-run-all -p start:cypress -r run:cypress",
    "typecheck": "tsc --noEmit -p src/frontend/tsconfig.json  --skipLibCheck && tsc --noEmit -p src/backend/tsconfig.json  --skipLibCheck"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css}": [
      "prettier --check",
      "eslint --max-warnings=0"
    ]
  },
