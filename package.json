    "@sentry/webpack-plugin": "^2.21.0",
    "amplitude-js": "^8.21.9",
    "axios": "^1.7.2",
    "babel-loader": "^9.1.3",
    "body-parser": "^1.20.0",
    "classnames": "^2.5.1",
    "constate": "^3.3.2",
    "date-fns": "3.6.0",
    "deep-equal": "^2.2.0",
    "dotenv": "^16.4.0",
