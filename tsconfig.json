{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "target": "es6",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "node_dist",
        "baseUrl": "./src/backend",
    },
    "include": ["./src/backend"],
    "exclude": ["./src/frontend"],
    "lib": ["es2015"]
  }
